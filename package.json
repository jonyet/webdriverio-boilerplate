{
  "name": "huge.webdriverio-boilerplate",
  "version": "3.0.0",
  "description": "boilerplate framework for e2e testing using node and webdriverio",
  "main": "wdio.conf.js",
  "scripts": {
    "test": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js",
    "test:e2e": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js --suite e2e",
    "test:integration": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js --suite integration",
    "test:local": "node_modules/.bin/wdio examples/Amazon/wdio.conf.local.js --suite dev"
  },
  "dependencies": {
    "@wdio/browserstack-service": "^5.4.11",
    "@wdio/cli": "^5.4.17",
    "@wdio/devtools-service": "^5.4.6",
    "@wdio/dot-reporter": "^5.4.15",
    "@wdio/firefox-profile-service": "^5.3.1",
    "@wdio/local-runner": "^5.4.17",
    "@wdio/mocha-framework": "^5.4.14",
    "@wdio/selenium-standalone-service": "^5.4.14",
    "@wdio/spec-reporter": "^5.4.15",
    "@wdio/sync": "^5.4.19",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.18.0",
    "chai": "3.5.0",
    "cheerio": "^1.0.0-rc.2",
    "chromedriver": "^2.46.0",
    "dotenv": "^2.0.0",
    "mocha": "^5.2.0",
    "selenium-standalone": "^6.15.4",
    "wdio-chromedriver-service": "^5.0.0",
    "webdriverio": "^5.4.17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonyet/webdriverio-boilerplate.git"
  },
  "author": "jonathan terry",
  "license": "MIT"
}
