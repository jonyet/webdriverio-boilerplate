{
  "name": "webdriverio-boilerplate",
  "version": "3.0.0",
  "description": "boilerplate framework for testing using node and webdriverio",
  "main": "wdio.conf.js",
  "engines": {
    "node": ">= 12.16.1"
  },
  "scripts": {
    "test": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js",
    "test:e2e": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js --suite e2e",
    "test:integration": "node_modules/.bin/wdio examples/Amazon/wdio.conf.js --suite integration",
    "test:local": "node_modules/.bin/wdio examples/Amazon/wdio.conf.local.js --suite dev"
  },
  "dependencies": {
    "@wdio/browserstack-service": "^7.9.0",
    "@wdio/cli": "^7.9.0",
    "@wdio/devtools-service": "^7.9.0",
    "@wdio/dot-reporter": "^7.9.0",
    "@wdio/local-runner": "^7.9.0",
    "@wdio/mocha-framework": "^7.9.0",
    "@wdio/selenium-standalone-service": "^7.8.0",
    "@wdio/spec-reporter": "^7.9.0",
    "@wdio/sync": "^7.9.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.3.4",
    "cheerio": "^1.0.0-rc.10",
    "chromedriver": "^92.0.0",
    "dotenv": "^10.0.0",
    "mocha": "^9.0.3",
    "selenium-standalone": "^7.1.0",
    "wdio-chromedriver-service": "^7.2.0",
    "webdriverio": "^7.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonyet/webdriverio-boilerplate.git"
  },
  "author": "jonathan terry",
  "license": "MIT"
}
